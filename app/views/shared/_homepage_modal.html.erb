<div class="modal fade" id="homepageModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Warning</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <h6>By continuing with Sign Up you will be signed out of your current session.</h6>
      </div>
      <div class="modal-footer">
        <button  id="modal-btn" type="button" class="btn btn-danger" data-dismiss="modal">
          <%= link_to "Continue", destroy_user_session_path(sign_up: true, user: user), method: :delete  %>
        </button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">
          Go back
        </button>
      </div>
    </div>
  </div>
</div>
