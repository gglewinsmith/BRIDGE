<%- if controller_name != 'sessions' %>
  <p class="edit-form-text center-form">Already have account? <%= link_to "Log in", new_session_path(resource_name) %><p/>
<% end -%>
<%- if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' %>
  <p class="edit-form-text center-form">Forgot your password? <%= link_to "Forgot password", new_password_path(resource_name) %><p/>
<% end -%>
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
  <p class="edit-form-text center-form">Didn't receive confirmation instructions? <%= link_to "Resend confirmation instructions", new_confirmation_path(resource_name) %><p/>
<% end -%>
<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <p class="edit-form-text center-form">Didn't receive unlock instructions? <%= link_to "Resend unlock instructions", new_unlock_path(resource_name) %><p/>
<% end -%>
<%- if devise_mapping.omniauthable? %>
<% end -%>

